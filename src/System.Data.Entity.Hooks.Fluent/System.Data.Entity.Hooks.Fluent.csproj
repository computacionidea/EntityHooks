<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>netstandard2.1</TargetFramework>
		<GeneratePackageOnBuild>false</GeneratePackageOnBuild>
		<Authors>Volodymyr Dombrovskyi</Authors>
		<GeneratePackageOnBuild>false</GeneratePackageOnBuild>
		<LangVersion>11</LangVersion>
		<RepositoryType>git</RepositoryType>
		<RepositoryUrl>https://github.com/computacionidea/EntityHooks.git</RepositoryUrl>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageProjectUrl>https://github.com/computacionidea/EntityHooks.git</PackageProjectUrl>
		<PackageId>Idea.Entity.Hooks.Fluent.netstd21</PackageId>
		<RootNamespace>System.Data.Entity.Hooks.Fluent</RootNamespace>
		<AssemblyName>System.Data.Entity.Hooks.Fluent</AssemblyName>
		<Version>1.2.1</Version>
		<Title>EntityFramework Hooks Fluent</Title>
		<description>EntityHooks provides an extension points for EF Code First DbContext in order to hook database operations.</description>
		<tags>ef entityframework entity dbcontext hook hooks codefirst</tags>
		<PackageIcon>icon.png</PackageIcon>
		<ContinuousIntegrationBuild>true</ContinuousIntegrationBuild>
	</PropertyGroup>
	
	<ItemGroup>
		<None Include="..\..\icon.png" Pack="true" PackagePath="icon.png" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="EntityFramework" Version="6.4.4" />
	</ItemGroup>
	<ItemGroup>
		<ProjectReference Include="..\System.Data.Entity.Hooks\System.Data.Entity.Hooks.csproj" />
	</ItemGroup>
</Project>

